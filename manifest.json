{
  "manifest_version": 2,

  "name": "Shiba Replacer",
  "description": "This extension replaces every image with a picture of a Shiba Inu.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "maxresdefault.png"
  } ,
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "web_accessible_resources": [

  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts.js"
      ]
    }

  ]

}